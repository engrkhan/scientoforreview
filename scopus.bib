	publisher = {Frontiers Media SA},
	issn = {26736225},
	language = {English},
	abbrev_source_title = {Front. Anim. Sci.},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 8; All Open Access, Gold Open Access, Green Open Access}
}

