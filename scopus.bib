@ARTICLE{Bretzinger20234429,
	author = {Bretzinger, L.F. and Tippenhauer, C.M. and Plenio, J.-L. and Heuwieser, W. and Borchardt, S.},
	title = {Effect of transition cow health and estrous expression detected by an automated activity monitoring system within 60 days in milk on reproductive performance of lactating Holstein cows},
