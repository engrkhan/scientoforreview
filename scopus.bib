@ARTICLE{Alkhtib2023,
	author = {Alkhtib, Ashraf and Burton, Emily and Wilson, Philippe B. and Scholey, Dawn and Bentley, James},
	title = {Effect of life cycle inventory choices and nutritional variation on carbon footprint of broiler meat production},
	year = {2023},
	journal = {Journal of Cleaner Production},
