@ARTICLE{Rah2021,
	author = {Rah, Hyung-Chul and Kim, Hyeon-Woong and Nasridinov, Aziz and Cho, Wan-Sup and Choi, Seo-Hwa and Yoo, Kwan-Hee},
	title = {Threshold effects of infectious disease outbreaks on livestock prices: Cases of african swine fever and avian influenza in south korea},
	year = {2021},
