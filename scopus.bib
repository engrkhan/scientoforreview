@ARTICLE{Abecia2023,
	author = {Abecia, J.A. and Canto, F. and Plaza, J. and Palacios, C.},
	title = {Body temperature, heart rate, and locomotor activity measured by bio-loggers before and after a progestogen+eCG treatment for artificial insemination in sheep: Effect of pregnancy},
	year = {2023},
