	affiliations = {ICAR-National Research Centre on Meat, Chengicherla, TS, Hyderabad, 500092, India; College of Veterinary Science, Sri Venkateswara Veterinary University, Andhra Pradesh, Tirupati, 517 502, India; Rajiv Gandhi Institute of Veterinary Education and Research, Kurumbapet, Puducherry, 605009, India},
