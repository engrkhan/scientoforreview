@ARTICLE{Cantor20226070,
	author = {Cantor, M.C. and Costa, J.H.C.},
	title = {Daily behavioral measures recorded by precision technology devices may indicate bovine respiratory disease status in preweaned dairy calves},
	year = {2022},
	journal = {Journal of Dairy Science},
	volume = {105},
