	keywords = {Animal Husbandry; Animals; Automation; Cattle; Dairying; Electricity; Farms; Female; Ireland; Lactation; Milk; Seasons; Time Factors; Water; water; agricultural land; animal; animal husbandry; automation; bovine; dairying; economics; electricity; female; Ireland; lactation; milk; physiology; procedures; season; statistics and numerical data; time factor},
